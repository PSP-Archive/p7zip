# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  all_c_code_config = debug
  7za_config = debug
endif
ifeq ($(config),release)
  all_c_code_config = release
  7za_config = release
endif

PROJECTS := all_c_code 7za

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

all_c_code:
ifneq (,$(all_c_code_config))
	@echo "==== Building all_c_code ($(all_c_code_config)) ===="
	@${MAKE} --no-print-directory -C . -f all_c_code.make config=$(all_c_code_config)
endif

7za: all_c_code
ifneq (,$(7za_config))
	@echo "==== Building 7za ($(7za_config)) ===="
	@${MAKE} --no-print-directory -C . -f 7za.make config=$(7za_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f all_c_code.make clean
	@${MAKE} --no-print-directory -C . -f 7za.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   all_c_code"
	@echo "   7za"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"